@model IEnumerable<WebWallet.Models.TransactionApi>

@{
    ViewData["Title"] = "Transaction History";
    var Version = TempData["Version"] ;
    var CreationDate = TempData["CreationDate"];
    var Name = TempData["Name"];
    var MerkleHash = TempData["MerkleHash"];
    var Input = TempData["Input"];
    var Amount = TempData["Amount"];
    var Output = TempData["Output"];
    var Delegate = TempData["Delegate"];
    var Signature = TempData["Signature"];
}

<h1>Transaction History</h1>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Version)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CreationDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MerkleHash)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Input)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Amount)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Output)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Delegate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Signature)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
      
                <tr>
                    <tr>
                    <td> @Version </td>
                    <td> @CreationDate </td>
                    <td> @Name  </td>
                    <td> @MerkleHash </td>
                    <td> @Input   </td>
                    <td> @Amount </td>
                    <td> @Output </td>
                    <td> @Delegate</td>
                    <td> @Signature</td>
                 
                </tr>
            
                </tr>
          
        
    </tbody>
</table>
