@model WebWallet.Models.TransactionRecord

@{
    ViewData["Title"] = "Transaction History";
    var Version = ViewData["Version"] ;
    var CreationDate = ViewData["CreationDate"];
    var MerkleHash = ViewData["MerkleHash"];
    var Input = ViewData["Input"];
    var Amount = ViewData["Amount"];
    var Output = ViewData["Output"];
    var Delegate = ViewData["Delegate"];
    var Signature = ViewData["Signature"];
}

<h1>Transaction History</h1>


<div>
  
    <hr />
    <dl class="row">
      
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Version)
        </dt>
        <dd class = "col-sm-10">
            @Version
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.CreationTime)
        </dt>
        <dd class = "col-sm-10">
            @CreationDate
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.MerkleHash)
        </dt>
        <dd class = "col-sm-10">
            @MerkleHash
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Input)
        </dt>
        <dd class = "col-sm-10">
            @Input
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Output)
        </dt>
        <dd class = "col-sm-10">
            @Output
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Amount)
        </dt>
        <dd class = "col-sm-10">
            @Amount
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.IsDelegating)
        </dt>
        <dd class = "col-sm-10">
            @Delegate
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Signature)
        </dt>
        <dd class = "col-sm-10">
            @Signature
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Details">Back</a>
</div>
